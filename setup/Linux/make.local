build: 	kernel/framework/include/buildid.h all
	sh build.sh

copy:	build
	cp -R prototype/* /

package:	build
		sh setup/Linux/mkpkg.sh

tarball:	build
		sh setup/Linux/mktarball.sh

deb:		build
		sh setup/Linux/mkdeb.sh

install:	copy
	cd /usr/lib/oss/build && sh install.sh
	sync
	soundoff && sync && soundon && echo "OSS has started OK"
