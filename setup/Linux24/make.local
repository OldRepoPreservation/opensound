build: 	kernel/framework/include/buildid.h all
	sh build.sh

copy:	build
	cp -R prototype/* /

package:	build
		cd setup/Linux && sh mkpkg.sh

tarball:	build
		sh setup/Linux/mktarball.sh

deb:		package
		alien --to-deb --scripts --keep-version oss-linux*.rpm

install:	copy
	cd /usr/lib/oss/build && sh install.sh
	sync
	soundoff && sync && soundon
